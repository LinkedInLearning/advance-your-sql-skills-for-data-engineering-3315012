{
  "editor.wordWrap": "on",
  "terminal.integrated.fontSize": 15,
  "sqltools.connections": [
    {
      "mysqlOptions": {
        "authProtocol": "default"
      },
      "previewLimit": 50,
      "server": "127.0.0.1",
      "port": 3306,
      "driver": "MariaDB",
      "name": "MariaDB",
      "database": "intro",
      "username": "root",
      "password": "mariadb"
    }
  ],
  "sqltools.useNodeRuntime": true,
  "terminal.integrated.env.linux": {
    "DBT_PROFILES_DIR": "/workspace"
  },
  "files.associations": {
    "*.sql": "jinja-sql"
  },
  "dbt.profilesDirOverride": "/workspace",
  "dbt.versionCheck": "neither"
}
